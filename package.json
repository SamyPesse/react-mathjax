    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.59.0",
    "gh-pages": "^0.12.0",
    "http-server": "^0.9.0",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
